{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nexport default {\n  name: \"article-item\",\n  props: {\n    article: {\n      type: Object,\n      required: true\n    }\n  },\n  created: function created() {// console.dir(JSON.stringify(this.article));\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA+CA;AACAA,sBADA;AAEAC;AACAC;AACAC,kBADA;AAEAC;AAFA;AADA,GAFA;AAQAC,SARA,qBAQA,CACA;AACA;AAVA","names":["name","props","article","type","required","created"],"sourceRoot":"src/components/article-item","sources":["index.vue"],"sourcesContent":["<template>\r\n    <div class=\"item-container\">\r\n        <van-cell>\r\n            <template #title>\r\n                <div class=\"title-box\">\r\n                    <!-- 标题 -->\r\n                    <span class=\"title\">{{ article.title }}</span>\r\n                    <!-- 单张图片 -->\r\n                    <van-image\r\n                        :src=\"article.cover.images[0]\"\r\n                        alt\r\n                        fit=\"cover\"\r\n                        class=\"thumb\"\r\n                        v-if=\"article.cover.type === 1\"\r\n                    />\r\n                </div>\r\n            </template>\r\n            <!-- label 区域的插槽 -->\r\n            <template #label>\r\n                <!-- 三张图片 -->\r\n                <div class=\"thumb-box\" v-if=\"article.cover.type === 3\">\r\n                    <van-image\r\n                        :src=\"item\"\r\n                        alt\r\n                        fit=\"cover\"\r\n                        class=\"thumb\"\r\n                        v-for=\"(item, i) in article.cover.images\"\r\n                        :key=\"i\"\r\n                    />\r\n                </div>\r\n                <div class=\"label-box\">\r\n                    <span>\r\n      