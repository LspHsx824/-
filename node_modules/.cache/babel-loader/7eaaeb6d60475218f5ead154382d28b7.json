{"ast":null,"code":"import \"core-js/modules/es.json.stringify.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nexport default {\n  name: \"article-item\",\n  props: {\n    article: {\n      type: Object,\n      required: true\n    }\n  },\n  created: function created() {\n    console.dir(JSON.stringify(this.article));\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA2CA;AACAA,sBADA;AAEAC;AACAC;AACAC,kBADA;AAEAC;AAFA;AADA,GAFA;AAQAC,SARA,qBAQA;AACAC;AACA;AAVA","names":["name","props","article","type","required","created","console"],"sourceRoot":"src/components/article-item","sources":["index.vue"],"sourcesContent":["<template>\r\n  <div class=\"item-container\">\r\n    <van-cell>\r\n      <template #title>\r\n        <div class=\"title-box\">\r\n          <!-- 标题 -->\r\n          <span>{{ article.title }}</span>\r\n          <!-- 单张图片 -->\r\n          <img\r\n            :src=\"article.cover.images[0]\"\r\n            alt\r\n            class=\"thumb\"\r\n            v-if=\"article.cover.type === 1\"\r\n          />\r\n        </div>\r\n            <!-- 三张图片 -->\r\n        <div class=\"thumb-box\" v-if=\"article.cover.type === 3\">\r\n            <img\r\n              :src=\"item\"\r\n              alt\r\n              class=\"thumb\"\r\n              v-for=\"(item, i) in article.cover.images\"\r\n              :key=\"i\"\r\n            />\r\n        </div>\r\n      </template>\r\n      <!-- label 区域的插槽 -->\r\n      <template #label>\r\n        <div class=\"label-box\">\r\n          <span>\r\n            作者 {{ article.aut_name }} &nbsp;&nbsp;\r\n            {{ article.comm_count }} 评论&nbsp;&nbsp; 发布日期\r\n            {{ article.pubdate }}\r\n          </span>\r\n\r\n          <!-- 关闭按钮 -->\r\n          <van-icon name=\"cross\" />\r\n        </div>\r\n      </template>\r\n    </van-cell>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  name: \"article-item\",\r\n  props: {\r\n    article: {\r\n      type: Object,\r\n      required: true,\r\n    },\r\n  },\r\n  created() {\r\n    console.dir(JSON.stringify(this.article));\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.label-box {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n.thumb {\r\n  // 矩形黄金比例：0.618\r\n  width: 113px;\r\n  height: 70px;\r\n  background-color: #f8f8f8;\r\n  object-fit: cover;\r\n}\r\n\r\n.title-box {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: flex-start;\r\n}\r\n\r\n.thumb-box {\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}